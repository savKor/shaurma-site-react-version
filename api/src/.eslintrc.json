{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "func-names":"off",
        "no-console":"off",
        "no-debugger":"off",
        "no-lonely-if":"off",
        "no-use-before-define":"off",
        "prefer-arrow-callback":"off",
        "no-plusplus":"off",
        "linebreak-style": ["error", "windows"]
    }
}
